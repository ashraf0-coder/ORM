

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id Int  @id @default (autoincrement())
  username String
  phone_number String?
  password String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()


  @@unique([username, password])
}